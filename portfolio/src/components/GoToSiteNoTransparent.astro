---
import GoToSite from '@components/GoToSite.astro';

const { color_palette } = await import('@portfolio/consts.json');

const background_color = color_palette.color_700;

interface Props {
	text: string;
	svg: string;
    url: string;
	target: string;
}

const { 
    text,
    svg,
    url,
	target,
} = Astro.props;
---


<GoToSite
    text={text}
    svg={svg}
    url={url}
	target={target}
    background_color={background_color}
></GoToSite>