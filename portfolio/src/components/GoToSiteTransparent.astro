---
import GoToSite from './GoToSite.astro';

interface Props {
	text: string;
	svg: string;
    url: string;
	color_50: string;
	color_100: string;
	color_200: string;
	color_300: string;
	color_400: string;
	color_500: string;
	color_600: string;
	color_700: string;
	color_800: string;
	color_900: string;
	color_950: string;
	color_white: string;
}

const { 
    text,
    svg,
    url,
	color_50,
	color_100,
	color_200,
	color_300,
	color_400,
	color_500,
	color_600,
	color_700,
	color_800,
	color_900,
	color_950,
	color_white,
} = Astro.props;
---


<GoToSite
    text={text}
    svg={svg}
    url={url}
    color_50 ={color_50}
    color_100={color_100}
    color_200={color_200}
    color_300={color_300}
    color_400={color_400}
    color_500={color_500}
    color_600={color_600}
    color_700="transparent"
    color_800={color_800}
    color_900={color_900}
    color_950="transparent"
    color_white={color_white}
></GoToSite>