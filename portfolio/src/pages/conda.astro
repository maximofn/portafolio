      <CodeBlockInputCell
        text={[
      '<span></span><span class="o">!</span>conda<span class="w"> </span>create<span class="w"> </span>-n<span class="w"> </span>mamba_env',
        ]}
        languaje='python'
      ></CodeBlockInputCell>






      
