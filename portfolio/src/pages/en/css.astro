      <span class="w">        </span><span class="n">return</span><span class="w"> </span><span class="nf">Node</span><span class="p">(</span><span class="n">data</span><span class="p">)</span>
      
      <span class="w">    </span><span class="err">#</span><span class="w"> </span><span class="n">Otherwise</span><span class="p">,</span><span class="w"> </span><span class="n">recur</span><span class="w"> </span><span class="n">down</span><span class="w"> </span><span class="n">the</span><span class="w"> </span><span class="n">tree</span>
      <span class="w">    </span><span class="n">if</span><span class="w"> </span><span class="n">data</span><span class="w"> </span><span class="o">&lt;</span><span class="w"> </span><span class="n">node</span><span class="o">.</span><span class="n">data</span><span class="o">:</span>
      <span class="w">        </span><span class="n">node</span><span class="o">.</span><span class="kc">left</span><span class="w"> </span><span class="o">=</span><span class="w"> </span><span class="nf">insert</span><span class="p">(</span><span class="n">node</span><span class="err">.</span><span class="kc">left</span><span class="p">,</span><span class="w"> </span><span class="n">data</span><span class="p">)</span>
      <span class="w">    </span><span class="n">else</span><span class="o">:</span>
