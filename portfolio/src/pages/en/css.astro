      <span class="w">    </span><span class="k">animation</span><span class="p">:</span><span class="w"> </span><span class="n">header-color</span><span class="w"> </span><span class="kc">linear</span><span class="p">;</span><span class="w">    </span><span class="n">animation-timeline</span><span class="p">:</span><span class="w"> </span><span class="nf">scroll</span><span class="p">(</span><span class="n">root</span><span class="p">);</span><span class="w">    </span><span class="n">animation-range</span><span class="p">:</span><span class="w"> </span><span class="mi">0</span><span class="w"> </span><span class="mi">200</span><span class="kt">px</span><span class="p">;</span><span class="err">```markdown</span>
