      '<span class="w">        </span><span class="o">&amp;</span><span class="n">lt</span><span class="p">;</span><span class="n">meta</span> <span class="nb">property</span><span class="o">=</span><span class="s2">&quot;og:description&quot;</span> <span class="n">content</span><span class="o">=</span><span class="s2">&quot;Descripción para las redes sociales&quot;</span><span class="o">/&amp;</span><span class="n">gt</span><span class="p">;</span>',
