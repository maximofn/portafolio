      '<span class="w">        </span><span class="o">&amp;</span><span class="n">lt</span><span class="p">;</span><span class="n">link</span> <span class="n">rel</span><span class="o">=</span><span class="s2">&quot;canonical&quot;</span> <span class="n">href</span><span class="o">=</span><span class="s2">&quot;path&quot;</span><span class="o">/&amp;</span><span class="n">gt</span><span class="p">;</span>',
