          <span class="k">yield</span> <span class="p">{opening_brace}</span><span class="s2">"es"</span><span class="p">:</span> <span class="s2">"El p√°jaro ha volado hoy"</span><span class="p">,</span> <span class="s2">"en"</span><span class="p">:</span> <span class="s2">"The bird has flown today"</span><span class="p">{closing_brace}</span>
          <span class="k">yield</span> <span class="p">{opening_brace}</span><span class="s2">"es"</span><span class="p">:</span> <span class="s2">"El pez ha nadado hoy"</span><span class="p">,</span> <span class="s2">"en"</span><span class="p">:</span> <span class="s2">"The fish has swum today"</span><span class="p">{closing_brace}</span>
          <span class="k">yield</span> <span class="p">{opening_brace}</span><span class="s2">"es"</span><span class="p">:</span> <span class="s2">"El caballo ha galopado hoy"</span><span class="p">,</span> <span class="s2">"en"</span><span class="p">:</span> <span class="s2">"The horse has galloped today"</span><span class="p">{closing_brace}</span>
          <span class="k">yield</span> <span class="p">{opening_brace}</span><span class="s2">"es"</span><span class="p">:</span> <span class="s2">"El cerdo ha corrido hoy"</span><span class="p">,</span> <span class="s2">"en"</span><span class="p">:</span> <span class="s2">"The pig has run today"</span><span class="p">{closing_brace}</span>
          <span class="k">yield</span> <span class="p">{opening_brace}</span><span class="s2">"es"</span><span class="p">:</span> <span class="s2">"El rat√≥n ha saltado hoy"</span><span class="p">,</span> <span class="s2">"en"</span><span class="p">:</span> <span class="s2">"The mouse has jumped today"</span><span class="p">{closing_brace}</span>
