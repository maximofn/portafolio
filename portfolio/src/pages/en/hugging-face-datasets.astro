          <span class="k">yield</span> <span class="p">{opening_brace}</span><span class="s2">"es"</span><span class="p">:</span> <span class="s2">"El le√≥n ha rugido hoy"</span><span class="p">,</span> <span class="s2">"en"</span><span class="p">:</span> <span class="s2">"The lion has roared today"</span><span class="p">{closing_brace}</span>
