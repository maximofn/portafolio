      <CodeBlockInputCell
        text={[
          '<span></span><span class="n">new_state</span> <span class="o">=</span> <span class="n">graph</span><span class="o">.</span><span class="n">get_state</span><span class="p">(</span><span class="n">config</span><span class="p">)</span>',
