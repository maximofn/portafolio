      '<span class="w">            </span><span class="p">)</span>',
      '<span class="w">            </span>',
      '<span class="w">            </span><span class="k">await</span> <span class="n">context</span><span class="o">.</span><span class="n">debug</span><span class="p">(</span><span class="sa">f</span><span class="s2">&quot;âœ… </span><span class="si">{</span><span class="n">partial_result</span><span class="si">}</span><span class="s2">&quot;</span><span class="p">)</span>',
      '<span class="w">    </span>',
      '<span class="w">    </span><span class="k">if</span> <span class="n">context</span><span class="p">:</span>',
