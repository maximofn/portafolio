      '<span class="w">                </span><span class="o"><</span><span class="n">img</span> <span class="n">src</span><span class="o">=</span><span class="s2">&quot;path&quot;</span> <span class="n">alt</span><span class="o">=</span><span class="s2">&quot;Texto alternativo&quot;</span><span class="o">/></span>',
