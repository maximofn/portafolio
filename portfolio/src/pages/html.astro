      '<span class="w">        </span><span class="o"><</span><span class="n">meta</span> <span class="n">name</span><span class="o">=</span><span class="s2">&quot;viewport&quot;</span> <span class="n">content</span><span class="o">=</span><span class="s2">&quot;width=device-width&quot;</span><span class="o">/></span>',
      '<span class="w">        </span><span class="o"><</span><span class="n">meta</span> <span class="n">name</span><span class="o">=</span><span class="s2">&quot;theme-color&quot;</span> <span class="n">content</span><span class="o">=</span><span class="s2">&quot;#09f&quot;</span><span class="o">/></span>',
