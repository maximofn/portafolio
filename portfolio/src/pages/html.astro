      '<span class="w">        </span><span class="o"><</span><span class="n">meta</span> <span class="nb">property</span><span class="o">=</span><span class="s2">&quot;og:title&quot;</span> <span class="n">content</span><span class="o">=</span><span class="s2">&quot;Título para las redes sociales&quot;</span><span class="o">/></span>',
      '<span class="w">        </span><span class="o"><</span><span class="n">meta</span> <span class="nb">property</span><span class="o">=</span><span class="s2">&quot;og:description&quot;</span> <span class="n">content</span><span class="o">=</span><span class="s2">&quot;Descripción para las redes sociales&quot;</span><span class="o">/></span>',
      '<span class="w">        </span><span class="o"><</span><span class="n">meta</span> <span class="nb">property</span><span class="o">=</span><span class="s2">&quot;og:image&quot;</span> <span class="n">content</span><span class="o">=</span><span class="s2">&quot;path&quot;</span><span class="o">/></span>',
