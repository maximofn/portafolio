          '<span></span><span class="o">!</span><span class="nb">cd</span><span class="w"> </span>notebook_git<span class="w"> </span><span class="o">&amp;&amp;</span><span class="w"> </span>ls<span class="w"> </span><span class="p">|</span><span class="w"> </span>grep<span class="w"> </span>README',
