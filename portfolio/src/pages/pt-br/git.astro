      <CodeBlockInputCell
        text={[
          '<span></span><span class="o">!</span><span class="nb">cd</span><span class="w"> </span>notebook_git<span class="w"> </span><span class="o">&amp;&amp;</span><span class="w"> </span>git<span class="w"> </span>add<span class="w"> </span>.gitignore<span class="w"> </span><span class="o">&amp;&amp;</span><span class="w"> </span>git<span class="w"> </span>commit<span class="w"> </span>-m<span class="w"> </span><span class="s2">"A√±adido .gitignore"</span>',
