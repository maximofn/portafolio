          '<span></span><span class="o">!</span><span class="nb">cd</span><span class="w"> </span>notebook_git<span class="w"> </span><span class="o">&amp;&amp;</span><span class="w"> </span><span class="nb">echo</span><span class="w"> </span><span class="s2">"print(\'He a√±adido una nueva linea\')"</span><span class="w"> </span>&gt;&gt;<span class="w"> </span>hola.py',
          '</span><span class="o">!</span><span class="nb">cd</span><span class="w"> </span>notebook_git<span class="w"> </span><span class="o">&amp;&amp;</span><span class="w"> </span>git<span class="w"> </span>status',
