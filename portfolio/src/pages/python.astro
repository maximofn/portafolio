      <CodeBlockInputCell
        text={[
      '<span></span><span class="n">diccionario</span> <span class="o">=</span> <span class="p">{</span>',
      '<span class="w">  </span><span class="s2">&quot;brand&quot;</span><span class="p">:</span> <span class="s2">&quot;Ford&quot;</span><span class="p">,</span>',
