const description = 'Guía completa de Python 2025: Tutorial desde cero hasta avanzado';
const keywords = 'python tutorial, aprender python, guía python, python desde cero, tipos de datos python, funciones python, clases python, programación python, python español, tutorial python completo';
const languaje = 'ES';
const image_path = 'https://images.maximofn.com/icon-python.webp';
const opening_brace = '{';
const closing_brace = '}';
